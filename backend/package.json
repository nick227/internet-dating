{
  "name": "internet-date-backend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate --schema prisma/schema && tsc",
    "start": "node dist/index.js",
    "test": "node --test --import tsx src/**/__tests__/**/*.test.ts",
    "test:feed": "node --test --import tsx src/registry/domains/feed/__tests__/feed.test.ts",
    "prisma:generate": "prisma generate --schema prisma/schema",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema",
    "prisma:studio": "prisma studio --schema prisma/schema",
    "migrate": "tsx scripts/runMigrations.ts",
    "openapi:emit": "tsx src/lib/openapi/emitOpenApi.ts",
    "seed:all": "tsx scripts/seedAll.ts",
    "admin:create": "tsx scripts/createAdmin.ts",
    "worker:jobs": "tsx src/workers/jobWorker.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "busboy": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "image-size": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@app/shared": "workspace:*",
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/busboy": "^1.5.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}
