{
  "name": "internet-date-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "knip",
    "analyze:deps": "madge --circular --extensions ts,tsx src",
    "analyze:bundle": "vite build --mode analyze",
    "openapi:types": "node scripts/generateOpenapiTypes.mjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:quick": "echo 'Open browser console and run: test-quick.js script'",
    "test:network": "echo 'Open test-network.html in browser'",
    "test:visual": "echo 'Open test-visual.html in browser'"
  },
  "dependencies": {
    "@types/busboy": "^1.5.4",
    "busboy": "^1.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@app/shared": "workspace:*",
    "@eslint/js": "^9.39.2",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^25.0.1",
    "knip": "^5.48.0",
    "madge": "^7.0.0",
    "openapi-typescript": "^6.7.5",
    "postcss": "^8.4.47",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  }
}
